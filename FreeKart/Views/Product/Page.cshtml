        <button type="button" data-bs-target="#demo" data-bs-slide-to="1"></button>
        <button type="button" data-bs-target="#demo" data-bs-slide-to="2"></button>
    </div>

    <!-- The slideshow/carousel -->
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="https://www.pngall.com/wp-content/uploads/4/Grocery-PNG-Pic.png" alt="Los Angeles" class="d-block" >
        </div>
        <div class="carousel-item">
            <img  src="https://static.vecteezy.com/system/resources/previews/023/489/783/non_2x/vegetable-farmer-market-counter-colorful-various-fresh-organic-healthy-vegetables-at-grocery-store-healthy-natural-food-concept-generative-ai-photo.jpg" alt="Chicago" class="d-block" >
        </div>
        <div class="carousel-item">
            <img  src="https://images.unsplash.com/photo-1583258292688-d0213dc5a3a8?q=80&w=1074&auto=format&fit=crop&ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" alt="New York" class="d-block" >
        </div>
    </div>

    <!-- Left and right controls/icons -->
    <button class="carousel-control-prev" style="background-color:grey" type="button" data-bs-target="#demo" data-bs-slide="prev">
        <span class="carousel-control-prev-icon"></span>
    </button>
    <button class="carousel-control-next" style="background-color:grey" type="button" data-bs-target="#demo" data-bs-slide="next">
        <span class="carousel-control-next-icon"></span>
    </button>
</div>


